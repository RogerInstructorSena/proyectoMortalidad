@model List<proyectoMortalidad.Models.TableViewModels.DepartamentosTVM>
@{
    ViewBag.Title = "Index";
}

<h2>Departamentos</h2>

<div class="row">

    <div class="col-lg-12">
        <table class="table-bordered">

            <tr>

                <th>Id</th>
                <th>Nombre Departamento</th>
            </tr>

            @foreach (var oElemento in Model)

            {
                <tr>
                    <td>@oElemento.codDptoPreguntaId</td>
                    <td>@oElemento.codDptoPreguntaNombre</td>

                </tr>
            }



        </table>


 
