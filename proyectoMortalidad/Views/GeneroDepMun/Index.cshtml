@model List<proyectoMortalidad.Models.TableViewModels.GeneroDepMunTVM>
@{
    ViewBag.Title = "Index";
}

<h2>Género Por Departamento Y Municipios</h2>

<div class="row">

    <div class="col-lg-12">
        <table class="table-bordered">

            <tr>

                <th style="text-align:left">Departamento</th>
                <th style="text-align:center">Municipio</th>
                <th style="text-align:center">Cantidad</th>
                <th style="text-align:center">Género</th>
            </tr>

            @foreach (var oElemento in Model)

            {
                <tr>
                    <td style="text-align:left">@oElemento.codDptoPreguntaNombre</td>
                    <td style="text-align:center">@oElemento.codMunicPreguntaId</td>
                    <td style="text-align:center">@oElemento.sexoPreguntaId</td>
                    <td style="text-align:center">@oElemento.sexoPreguntaNombre</td>

                </tr>
            }



        </table>

</div>
</div>